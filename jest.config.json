{
    "testRegex": "*.test.js$",
    "collectCoverage": true,
    "coverageReporters": ["lcov"],
    "collectCoverageFrom": [
        "./src/ui/modules/**/*.ts"
    ],
    "coverageThreshold": {
     "global": {
     "branches": 0,
     "functions": 0,
     "lines": 0,
     "statements": 0
     }
    },
    "moduleDirectories": ["node_modules", "src"]
   }